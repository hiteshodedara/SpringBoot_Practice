package Paracticeprogs;

public class areaorperimetercircle {
	public static void main(String[]a) {
		
		float pia=3.14159f;
		float radius=7.5f;
		float area=pia*radius*radius;
		float perimeter=2*pia*radius;

		System.out.print("Perimeter is= ");
		System.out.println(perimeter);
		System.out.print("Radius is= ");
		System.out.println(area);
		
	}
}
